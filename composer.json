    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Mauricio Blint",
            "email": "hi@mauricioblint.com",
            "homepage": "http://mauricioblint.com",
            "role": "Developer"
        }
    ],
    "replace": {
        "bfoxwell/image-palette": "*"
    },
    "require": {
        "php": ">=5.4.0",
